{
    "name": "gavra-android-root",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "mcp:install": "pnpm --prefix supabase-mcp-official install",
        "mcp:build": "pnpm --prefix supabase-mcp-official build",
        "mcp:dev": "pnpm --prefix supabase-mcp-official --filter @supabase/mcp-server-supabase dev",
        "mcp:run": "node scripts/run-official-mcp.js",
        "mcp:cli:start": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/start-supabase.ps1",
        "mcp:dev:all": "node scripts/dev-all.js",
        "mcp:health": "node scripts/check-mcp-health.js"
    }
}