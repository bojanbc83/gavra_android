{
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3"
  },
  "max-concurrent-downloads": 3,
  "max-concurrent-uploads": 3,
  "default-shm-size": "64M",
  "default-ulimits": {
    "nofile": {
      "Name": "nofile",
      "Hard": 1024,
      "Soft": 1024
    }
  },
  "userland-proxy": false,
  "experimental": false,
  "live-restore": true,
  "registry-mirrors": [],
  "insecure-registries": [],
  "debug": false
}