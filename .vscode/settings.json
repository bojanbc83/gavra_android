{
  "firebase.emulators.auth.port": 9099,
  "emmet.includeLanguages": {
    "firerules": "javascript"
  },
  "json.schemas": [
    {
      "url": "./.vscode/firebase_config_schema.json",
      "fileMatch": [
        "firebase.json"
      ]
    },
    {
      "url": "./.vscode/firebase_config_schema.json",
      "fileMatch": [
        "firestore.indexes*.json"
      ]
    }
  ],
  "files.associations": {
    "firestore.rules": "firerules",
    "firestore.rules.production": "firerules",
    "firestore.indexes.json": "json",
    "firebase.json": "jsonc",
    "*.rules": "firerules"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "firebase.emulators.firestore.port": 8080
}
